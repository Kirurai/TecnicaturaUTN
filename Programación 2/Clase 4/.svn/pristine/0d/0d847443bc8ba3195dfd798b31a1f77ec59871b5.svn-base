package urz√∫a_marcos;

import java.util.ArrayList;

/**
 *
 * @author Marcos
 */
public class Sucursal {
    private ArrayList<Producto> deposito = new ArrayList();
    private String nombre;
    private int totalStock;
    private double totalValores;
    private boolean pedidoPendiente;

    public Sucursal(String nombre, int totalStock, double totalValores) {
        this.pedidoPendiente = false;
        this.nombre = nombre;
        this.totalStock = totalStock;
        this.totalValores = totalValores;
    }

    public ArrayList<Producto> getDeposito() {
        return deposito;
    }
    public void setDeposito(ArrayList<Producto> deposito) {
        this.deposito = deposito;
    }
    public String getNombre() {
        return nombre;
    }
    public void setNombre(String nombre) {
        this.nombre = nombre;
    }
    public int getTotalStock() {
        return totalStock;
    }
    public void setTotalStock(int totalStock) {
        this.totalStock = totalStock;
    }
    public double getTotalValores() {
        return totalValores;
    }
    public void setTotalValores(double totalValores) {
        this.totalValores = totalValores;
    }

    public boolean isPedidoPendiente() {
        return pedidoPendiente;
    }
    public void setPedidoPendiente(boolean pedidoPendiente) {
        this.pedidoPendiente = pedidoPendiente;
    }
    
    
    
    
    
}
