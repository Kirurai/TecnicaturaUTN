package mercado_simi;

import java.util.ArrayList;

/**
 *
 * @author Usuario
 */
public class Sucursal {

    private String nombre;
    private String direccion;
    private String tipo;
    private int telefono;
    private ArrayList<Producto> stock = new ArrayList<>();

    public Sucursal() {
    }

    public Sucursal(String nombre, String direccion, String tipo, int telefono) {
        this.nombre = nombre;
        this.direccion = direccion;
        this.tipo = tipo;
        this.telefono = telefono;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public String getDireccion() {
        return direccion;
    }

    public void setDireccion(String direccion) {
        this.direccion = direccion;
    }

    public String getTipo() {
        return tipo;
    }

    public void setTipo(String tipo) {
        this.tipo = tipo;
    }

    public int getTelefono() {
        return telefono;
    }

    public void setTelefono(int telefono) {
        this.telefono = telefono;
    }

    public ArrayList<Producto> getStock() {
        return stock;
    }

    public void setStock(ArrayList<Producto> stock) {
        this.stock = stock;
    }
}
